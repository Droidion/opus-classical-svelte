{
  "name": "opus-classical-svelte",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "bun@1.1.34",
  "engines": {
    "bun": ">=1.1.34"
  },
  "engineStrict": true,
  "scripts": {
    "dev": "bun run --bun vite dev",
    "build": "bun run --bun vite build",
    "preview": "bun run --bun vite preview",
    "check": "bun run svelte-kit sync && bun run svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "bun run svelte-kit sync && bun run svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "bun run eslint .",
    "lint:fix": "bun run eslint . --fix",
    "test": "bun test",
    "test:ci": "bun test --run"
  },
  "dependencies": {
    "@neondatabase/serverless": "^0.10.1",
    "drizzle-orm": "^0.36.0",
    "fuse.js": "^7.0.0",
    "ky": "^1.7.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.8.0",
    "@sveltejs/kit": "^2.7.5",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@types/bun": "^1.1.13",
    "@types/eslint": "^9.6.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "eslint-plugin-format": "^0.1.2",
    "eslint-plugin-svelte": "^2.46.0",
    "globals": "^15.12.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "prettier-plugin-svelte": "^3.2.7",
    "svelte": "^5.1.9",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-check": "^4.0.5",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "vite": "^5.4.10"
  }
}
